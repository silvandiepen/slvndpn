		"@nuxtjs/google-analytics": "^2.2.3",
		"@nuxtjs/pwa": "^3.0.0-beta.19",
		"@octokit/rest": "^17.1.0",
		"@sil/markdown": "^1.0.1",
		"@vue/composition-api": "^0.5.0",
		"apollo-boost": "^0.4.7",
		"apollo-cache-inmemory": "^1.6.5",
		"apollo-client": "^2.6.8",
		"apollo-link": "^1.2.13",
		"apollo-link-http": "^1.5.16",
		"axios": "^0.19.2",
		"graphql": "^14.6.0",
		"graphql-tag": "^2.10.3",
		"nuxt": "^2.15.8",
		"nuxt-graphql-request": "^1.0.1",
		"vue-apollo": "^3.0.3",
		"vue-async-computed": "^3.8.2",
		"vuex-persist": "^2.2.0"
	},
	"devDependencies": {
